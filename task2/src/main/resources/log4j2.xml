<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="GeomCalculator">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{dd.M.yy HH:mm:ss.SSS} %highlight{%-5level} %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>
